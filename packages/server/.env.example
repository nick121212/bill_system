#######################
# 应用程序基本配置
#######################
# 服务器运行端口
PORT=3000
# 运行环境（development/production）
NODE_ENV=development
# 哈希加密密钥
HASH_SECRET=your_hash_secret

#######################
# 数据库配置
#######################
# MySQL主机地址
DB_HOST=localhost
# MySQL端口
DB_PORT=3306
# MySQL用户名
DB_USER=root
# MySQL密码
DB_PASSWORD=your_password
# MySQL数据库名
DB_NAME=bill_zg

#######################
# Redis配置
#######################
# Redis主机地址
REDIS_HOST=localhost
# Redis端口
REDIS_PORT=6379
# Redis用户名（可选）
REDIS_USERNAME=
# Redis密码（可选）
REDIS_PASSWORD=
# Redis数据库索引
REDIS_DATABASE=1
# Redis键前缀，用于区分不同应用
REDIS_KEY_PREFIX=bill_system

#######################
# JWT认证配置
#######################
# JWT签名密钥
JWT_SECRET=your-secret-key
# JWT访问令牌有效期（秒）
JWT_ACCESS_TOKEN_TTL=3600

#######################
# Swagger API文档配置
#######################
# Swagger站点标题
SWAGGER_SITE_TITLE=The NestJs Authentication API
# Swagger文档标题
SWAGGER_DOC_TITLE=NestJs Authentication
# Swagger文档描述
SWAGGER_DOC_DESCRIPTION=The NestJs Authentication API
# Swagger文档版本
SWAGGER_DOC_VERSION=1.0
